{
  "private": true,
  "name": "pelagrino-backend",
  "version": "0.0.1",
  "description": "Pelagrino Backend",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run build:server && yarn run build:admin",
    "build:server": "tsc -p tsconfig.server.json",
    "build:admin": "medusa-admin build",
    "watch": "tsc --watch",
    "test": "jest",
    "seed": "medusa seed -f ./data/seed.json",
    "start": "yarn run build && NODE_OPTIONS=--max-old-space-size=3072 node index.js",
    "start:custom": "yarn run build && node --preserve-symlinks index.js",
    "dev": "medusa develop"
  },
  "dependencies": {
    "@medusajs/admin": "7.1.12",
    "@medusajs/cache-redis": "1.8.8",
    "@medusajs/event-bus-local": "1.9.5",
    "@medusajs/event-bus-redis": "1.8.8",
    "@medusajs/file-local": "^1.0.1",
    "@medusajs/medusa": "1.20.0",
    "@medusajs/medusa-cli": "^1.3.16",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "immutability-helper": "^3.1.1",
    "medusa-file-s3": "^1.4.0",
    "medusa-fulfillment-manual": "^1.1.38",
    "medusa-interfaces": "^1.3.8",
    "medusa-payment-manual": "^1.0.23",
    "medusa-plugin-filestorage-local": "^0.1.2",
    "react-query": "^3.39.3",
    "react-table": "^7.8.0",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@hookform/error-message": "^2.0.1",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-portal": "^1.0.3",
    "@segment/analytics-next": "^1.53.1",
    "@swc/core": "^1.3.70",
    "babel-preset-medusa-package": "^1.1.20",
    "clsx": "^2.0.0",
    "css-loader": "^6.8.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "postcss-loader": "^7.3.3",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^4.4.1",
    "react-jwt": "^1.2.0",
    "source-map-loader": "^4.0.1",
    "swc-loader": "^0.2.3",
    "typescript": "^5.1.6"
  }
}
